{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ES6",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "moduleResolution": "Node"
  },
  "include": ["**/*.ts", "**/*.tsx"]
}